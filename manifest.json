{
  "manifest_version": 3,
  "name": "Prime Speed Controller",
  "version": "1.0",
  "description": "Adjust Prime Video Speed",
  "permissions": ["activeTab", "webRequest"],
  "content_scripts": [
    {
      "matches": ["*://*.primevideo.com/*"],
      "js": ["./src/content/index.js"]
    }
  ],
  "background": {
    "js": ["./src/background/index.js"]
  }
}
